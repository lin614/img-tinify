{
    "globals": {
        "QUnit": true
    },
    "rules": {
        "indent": [
            2,
            "tab",
            {
                "SwitchCase": 1
            }
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "curly": 2,
        "eqeqeq": 2,
        "no-constant-condition": 0,
        "no-unreachable": 0
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "extends": "eslint:recommended"
}